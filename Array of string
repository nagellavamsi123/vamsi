#include <stdio.h>
#include<string.h>
#include<conio.h>
void main()
{
    char a[10][10];
    int b;
    printf("enter no of names");
    scanf("%d",&b);
    printf("enter the names");
    for(int i=0;i<b;i++)
    {
        scanf("%s",a[i]);
     }
     printf("the legends names are");
     for (int i=0;i<b;i++)
     {
         printf("\n the no.of names are:%d %s",i+1,a[i]);
     }
}
     
     
