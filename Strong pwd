#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
    char pass[15];
    printf("Enter password:");
    gets(pass);
    int l=strlen(pass);
    int up=0,lw=0,sp=0,dg=0;
    if(l>8)
    {
      for(int i=0;i<=l;i++)
      {
          if(isupper(pass[i]))
            up++;
          else if(islower(pass[i]))
            lw++;
          else if(isdigit(pass[i]))
            dg++;
          else
            sp++;
      }
      if(up>0 && lw>0 && dg>0 && sp>0)
      {
          printf("Password is strong");
      }
      else
      {
          printf("password is weak");
      }
    }
    else
    {
        printf("Weak password due to less characters");
    }
}
